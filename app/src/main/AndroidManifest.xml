<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nsu.vcho">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".Application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activity.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.GuideActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.LoginActivity"></activity>
        <activity android:name=".Activity.UserSettingActivity"></activity>
        <activity android:name=".Activity.UserLoginSetting"></activity>
        <activity android:name=".Activity.UserInfoActivity"></activity>
        <activity android:name=".Activity.AddTextActivity"></activity>
        <activity android:name=".Activity.LaunchActivity"></activity>
    </application>
</manifest>